#JSGF V1.0;

grammar comedores;

public <comedores> =
			[<deseo>]
			[<info>]
			<asunto> {this.informacion=$asunto};

<deseo> = quiero | me gustaria | [yo] necesito | [yo] queria;

<info> = informacion [sobre | de];

<asunto> = horario | contacto | telefono | menus | precio | localizacion | donde se encuentra;
